<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <layout class="ch.qos.logback.classic.PatternLayout">
      <Pattern>%d{HH:mm:ss.SSS} [%thread] %-5level zzz %logger{36} zzzzz- %msg%n</Pattern>
    </layout>
  </appender>
  
  <logger name="ru.roseurobank.c24.testvelocity" level="TRACE"/>
  

  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>